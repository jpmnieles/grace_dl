<?xml version="1.0" ?>

<robot name="robot">

    <!-- Links -->
    <link name="world"/>

    <link name="torso">
        <visual>
            <geometry>
                <box size="0.26 0.30 0.6"/>
            </geometry>
            <origin xyz="0.01767 0 -0.267155"/>
            <material name="robot_model">
                <color rgba="0.3 0.3 0.3 0.4"/>
            </material>
        </visual>
    </link>

    <link name="realsense">
        <visual>
            <geometry>
                <box size="0.09 0.025 0.025"/>
            </geometry>
            <origin xyz="0.0325 0 -0.01485" rpy="0 0 0"/>
            <material name="robot_camera">
                <color rgba="0.0 0.1 0.3 0.6"/>
            </material>
        </visual>
    </link>

    <link name="neck">
        <visual>
            <geometry>
                <cylinder length="0.14" radius="0.045"/>
            </geometry>
            <origin xyz="0 0 0.07"/>
            <material name="robot_model">
                <color rgba="0.3 0.3 0.3 0.4"/>
            </material>
        </visual>
    </link>

    <link name="neck_py"/>

    <link name="head">
        <visual>
            <geometry>
                <box size="0.21 0.115 0.20"/>
            </geometry>
            <origin xyz="0.0 0 0.06"/>
            <material name="robot_model">
                <color rgba="0.3 0.3 0.3 0.4"/>
            </material>
        </visual>
    </link>

    <link name="eyes"/>

    <link name="lefteye">
        <visual>
            <geometry>
                <sphere radius="0.005"/>
            </geometry>
            <origin xyz="0 0 0"/>
            <material name="robot_camera">
                <color rgba="0.0 0.1 0.3 0.6"/>
            </material>
        </visual>
    </link>

    <link name="righteye">
        <visual>
            <geometry>
                <sphere radius="0.005"/>
            </geometry>
            <origin xyz="0 0 0"/>
            <material name="robot_camera">
                <color rgba="0.0 0.1 0.3 0.6"/>
            </material>
        </visual>
    </link>

    <link name="leftcamera"/>

    <link name="rightcamera"/>

    <!-- Joints -->

    <joint name="chest_camera" type="fixed">
        <parent link="world"/>
        <child link="realsense"/>
        <origin xyz="0 0 0.44739" rpy="-1.5708 0 -1.5708"/>
    </joint>

    <joint name="torso" type="fixed">
        <parent link="realsense"/>
        <child link="torso"/>
        <origin xyz="0.0325 -0.05692 -0.12234" rpy="1.5708 -1.5708 0"/> <!--"-0.12234 -0.0325 0.05692" rpy="1.5708 -1.5708 0"-->
    </joint>

    <joint name="neck_pitch" type="revolute">
        <parent link="torso"/>
        <child link="neck_py"/>
        <axis xyz="0 -1 0"/>
        <origin xyz="0 0 0"/>
        <limit effort="1" lower="-1" upper="1" velocity="1"/>
    </joint>

    <joint name="neck_yaw" type="revolute">
        <parent link="neck_py"/>
        <child link="neck"/>
        <axis xyz="0 0 -1"/>
        <origin xyz="0 0 0"/>
        <limit effort="1" lower="-1" upper="1" velocity="1"/>
    </joint>

    <joint name="head_pitch" type="revolute">
        <parent link="neck"/>
        <child link="head"/>
        <axis xyz="0 -1 0"/>
        <origin xyz="0 0 0.13172"/>
        <limit effort="1" lower="-1" upper="1" velocity="1"/>
    </joint>

    <joint name="eyes_pitch" type="revolute">
        <parent link="head"/>
        <child link="eyes"/>
        <axis xyz="0 -1 0"/>
        <origin xyz="0.08492 0 0.05186"/>
        <limit effort="1" lower="-1" upper="1" velocity="1"/>
    </joint>

    <joint name="lefteye_yaw" type="revolute">
        <parent link="eyes"/>
        <child link="lefteye"/>
        <axis xyz="0 0 -1"/>
        <origin xyz="0 0.02895 0"/>  <!--rpy="0 0 -0.0289"-->
        <limit effort="1" lower="-1" upper="1" velocity="1"/>
    </joint>

    <joint name="righteye_yaw" type="revolute">
        <parent link="eyes"/>
        <child link="righteye"/>
        <axis xyz="0 0 -1"/>
        <origin xyz="0 -0.02895 0"/>  <!--rpy="0 0 0.0289"-->
        <limit effort="1" lower="-1" upper="1" velocity="1"/>
    </joint>

    <joint name="lefteye_cam" type="fixed">
        <parent link="lefteye"/>
        <child link="leftcamera"/>
        <origin xyz="0.015 0 0" rpy="-1.5708 0 -1.5708"/>
    </joint>

    <joint name="righteye_cam" type="fixed">
        <parent link="righteye"/>
        <child link="rightcamera"/>
        <origin xyz="0.015 0 0" rpy="-1.5708 0 -1.5708"/>
    </joint>


</robot>